@prefix map: <#> .
@prefix db: <> .
@prefix vocab: <vocab/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schm: <http://schema.org/> .
@prefix morelab: <http://www.morelab.deusto.es/ontologies/proactive#> .

map:database a d2rq:Database;
	d2rq:jdbcDriver "org.postgresql.Driver";
	d2rq:jdbcDSN "jdbc:postgresql://localhost/postgres";
	d2rq:username "postgres";
	d2rq:password "postgres";
	.

# Table action
map:action a d2rq:ClassMap;
	d2rq:dataStorage map:database;
	d2rq:uriPattern "action/@@action.id@@";
	d2rq:class vocab:action;
	d2rq:classDefinitionLabel "action";
	.
map:action__label a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:action;
	d2rq:property rdfs:label;
	d2rq:pattern "action #@@action.id@@";
	.
map:action_id a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:action;
	d2rq:property vocab:action_id;
	d2rq:propertyDefinitionLabel "action id";
	d2rq:column "action.id";
	d2rq:datatype xsd:integer;
	.
map:action_action_name a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:action;
	d2rq:property vocab:action_action_name;
	d2rq:propertyDefinitionLabel "action action_name";
	d2rq:column "action.action_name";
	.
map:action_rating a d2rq:PropertyBridge;
	d2rq:belongsToClassMap map:action;
	d2rq:property vocab:action_rating;
	d2rq:propertyDefinitionLabel "action rating";
	d2rq:column "action.rating";
	d2rq:datatype xsd:double;
	.

